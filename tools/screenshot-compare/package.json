{
  "name": "screenshot-compare",
  "version": "1.0.0",
  "description": "Compare two screenshots using the Gemini API to identify visual differences",
  "type": "module",
  "bin": {
    "screenshot-compare": "dist/index.js"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "build": "npm run clean && ncc build src/index.ts -o dist -m -e node-fetch -e fetch-blob -e formdata-polyfill -e google-auth-library",
    "check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "dev": "tsx src/index.ts",
    "prepare": "npm run build",
    "start": "node dist/index.js"
  },
  "keywords": [
    "screenshot",
    "gemini",
    "comparison",
    "ui-testing",
    "cli"
  ],
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.39.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vercel/ncc": "^0.38.4",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0"
  }
}
