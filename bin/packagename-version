#!/usr/bin/env bash

require() { hash "$@" || exit 127; }

require adb

if [ -z "$1" ]; then
  echo "usage: $(basename "$0") packageName"
  exit 1
fi

echo "packageName=$1"
adb shell dumpsys package "$1" | grep versionCode | awk '{print $1}'
adb shell dumpsys package "$1" | grep versionName | awk '{print $1}'
