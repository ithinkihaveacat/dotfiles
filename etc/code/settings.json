{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[fish]": {
    "editor.wordWrap": "off"
  },
  "[markdown]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    }
  },
  "breadcrumbs.enabled": true,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.autoClosingBrackets": "never",
  "editor.autoIndent": "full",
  "editor.cursorSurroundingLines": 5,
  "editor.folding": true,
  "editor.fontFamily": "\"Cascadia Code\", \"Source Code Pro\"",
  "editor.fontLigatures": "'ss19'",
  "editor.fontSize": 13,
  "editor.formatOnSave": true,
  "editor.lineHeight": 0,
  "editor.quickSuggestionsDelay": 600,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [80, 100],
  "editor.suggest.localityBonus": true,
  "editor.tabSize": 2,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 100,
  "emmet.triggerExpansionOnTab": true,
  "eslint.enable": false,
  "explorer.confirmDelete": false,
  "explorer.decorations.colors": false,
  "extensions.ignoreRecommendations": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.enabled": true,
  "githubPullRequests.telemetry.enabled": false,
  "go.useLanguageServer": true,
  "html.autoClosingTags": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "markdown.preview.fontFamily": "'Inter', sans-serif",
  "prettier.jsxBracketSameLine": true,
  "prettier.proseWrap": "always",
  "prettier.requireConfig": true,
  "rewrap.wrappingColumn": 80,
  "search.smartCase": true,
  "search.useIgnoreFiles": true,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.lineHeight": 1.2,
  "typescript.implementationsCodeLens.enabled": false,
  "typescript.referencesCodeLens.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Solarized Dark",
  "workbench.fontAliasing": "auto",
  "workbench.startupEditor": "newUntitledFile",
  "files.associations": {
    "*.hbs": "handlebars"
  },
  "editor.semanticHighlighting.enabled": true,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.indentation": false,
  "cloudcode.updateChannel": "dogfood",
  "geminicodeassist.updateChannel": "dogfood",
  "geminicodeassist.project": "aipp-internal-testing",
  "diffEditor.codeLens": true,
  "geminicodeassist.recitation.maxCitedLength": 0
}